{
  "name": "meteor",
  "version": "1.0.1",
  "description": "every thing for speed",
  "keywords": [
    "meteor",
    "vscode"
  ],
  "author": "shenjiaolong",
  "homepage": "https://www.80fight.cn/",
  "license": "ISC",
  "main": "dist/client",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "publishConfig": {
    "registry": "https://registry.npm.taobao.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/jiaolong1021/meteor.git"
  },
  "scripts": {
    "prebuild": "cd ../.. && npm run build",
    "build": "npm run build:node",
    "build:node": "node scripts/build-node",
    "watch": "npm run build:node -- --watch",
    "prepack:node": "npm run prebuild && npm run build:node -- --minify",
    "release": "npm run release:node",
    "release:node": "npm run prepack:node && npm run publish",
    "publish": "vsce package",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "*",
    "onLanguage:vue"
  ],
  "contributes": {
    "commands": [
      {
        "command": "hello",
        "title": "hello"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "axios": "^0.21.1",
    "esbuild": "0.15.18",
    "esbuild-plugin-copy": "^2.0.2",
		"vsce": "latest"
	},
  "gitHead": "b1cc40df44b9cd73ba96d8b7f0369cc3ce690c45"
}
